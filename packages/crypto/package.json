{
  "name": "@atlas/crypto",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./double-ratchet": {
      "types": "./dist/double-ratchet.d.ts",
      "import": "./dist/double-ratchet.js"
    },
    "./dpop": {
      "types": "./dist/dpop.d.ts",
      "import": "./dist/dpop.js"
    },
    "./http-signatures": {
      "types": "./dist/http-signatures.d.ts",
      "import": "./dist/http-signatures.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "libsodium-wrappers": "^0.7.13"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.14",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
