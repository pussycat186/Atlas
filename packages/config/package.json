{
  "name": "@atlas/config",
  "version": "0.0.0",
  "description": "Atlas configuration utilities for live URLs and environment management",
  "type": "module",
  "main": "./dist/node.js",
  "module": "./dist/node.js",
  "browser": "./dist/browser.js",
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.node.json && tsc -p tsconfig.browser.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  }
}
