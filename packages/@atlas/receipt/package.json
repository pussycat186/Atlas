{
  "name": "@atlas/receipt",
  "version": "0.1.0",
  "description": "Atlas RFC 9421 HTTP Message Signatures for verifiable receipts",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cli": "node dist/cli.js"
  },
  "bin": {
    "atlas-receipt": "./dist/cli.js"
  },
  "dependencies": {
    "jose": "^5.1.3",
    "node-forge": "^1.3.1",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/node-forge": "^1.3.11",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  }
}